[
  {
    "environment": [
      {
        "name": "DJANGO_SECRET_KEY",
        "value": "12345"
      },
      {
        "name": "DJANGO_SETTINGS_MODULE",
        "value": "settings.common"
      },
      {
        "name": "SERVICE_NAME",
        "value": "mcp-server"
      }
    ],
    "essential": true,
    "links": [
      "mysql",
      "gearmand"
    ],
    "mountPoints": [
      {
