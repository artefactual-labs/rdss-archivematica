[
  {
    "cpu": 256,
    "environment": [
      {
        "name": "SERVICE_NAME",
        "value": "redis"
      }
    ],
    "essential": true,
    "image": "${image_url}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}"
      }
    },
    "memory": 256,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 6379,
        "hostPort": 6379
      }
    ],
    "user": "redis"
  }
]
